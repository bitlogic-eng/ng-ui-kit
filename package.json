{
  "name": "demo-lib",
  "version": "0.1.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": "https://github.com/bitlogic-eng/ng-ui-kit.git",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run prebuild-ucc && npm run prebuild-u21 && ng build bitlogic --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "symlink": "npm link ./dist/bitlogic",
    "build:dev": "ng build bitlogic",
    "build:dev:w": "ng build bitlogic --watch",
    "publish": "npm publish ./dist/bitlogic",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook",
    "deploy-storybook": "npx angular-cli-ghpages --dir=storybook-static",
    "prebuild-ucc": "node-sass --importer ./node_modules/node-sass-package-importer/dist/cli.js ./projects/bitlogic/assets/scss/theme.scss ./projects/bitlogic/assets/prebuild-themes/theme-ucc.css --output-style compressed",
    "prebuild-u21": "node-sass --importer ./node_modules/node-sass-package-importer/dist/cli.js ./projects/bitlogic/assets/scss/theme.scss ./projects/bitlogic/assets/prebuild-themes/theme-u21.css --output-style compressed",
    "version:pat": "cd ./projects/bitlogic && npm version patch",
    "version:min": "cd ./projects/bitlogic && npm version minor",
    "version:maj": "cd ./projects/bitlogic && npm version major"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.4",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "~11.0.4",
    "@angular/compiler": "~11.0.4",
    "@angular/core": "~11.0.4",
    "@angular/forms": "~11.0.4",
    "@angular/material": "^11.0.3",
    "@angular/platform-browser": "~11.0.4",
    "@angular/platform-browser-dynamic": "~11.0.4",
    "@angular/router": "~11.0.4",
    "@ngu/carousel": "^3.0.2",
    "bootstrap": "^4.6.0",
    "fuse.js": "^6.4.6",
    "hammerjs": "^2.0.8",
    "jquery": "^3.6.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.5",
    "@angular/cli": "~11.0.4",
    "@angular/compiler-cli": "~11.0.4",
    "@angular/elements": "^12.1.3",
    "@babel/core": "^7.14.8",
    "@compodoc/compodoc": "^1.1.14",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/angular": "^6.3.6",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@webcomponents/custom-elements": "^1.4.3",
    "angular-cli-ghpages": "^1.0.0-rc.2",
    "babel-loader": "^8.2.2",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.0",
    "node-sass": "^7.0.0",
    "node-sass-package-importer": "^5.3.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
